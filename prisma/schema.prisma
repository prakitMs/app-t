generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  
}
model User {
  id         String   @id @default(uuid()) @db.Uuid
  email      String   @unique
  first_name String
  last_name  String
  password   String
  is_active  Boolean  @default(true)
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
